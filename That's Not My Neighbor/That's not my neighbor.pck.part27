	game_mode_03_button.disabled = false
	game_mode_04_button.disabled = false
	game_mode_05_button.disabled = false

func _play_sfx(n:int) -> void:
	match n:
		0:
			sfx_select.play()
		1:
			sfx_accept.play()
		2:
			sfx_pick_up_folder.play()
		3:
			sfx_check_write.play()
		_:
			pass

