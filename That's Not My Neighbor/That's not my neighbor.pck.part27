func _set_doppel_info() -> void:
	var next_info = "a"
	for letter in Global.doppels_info:
		if letter == "*":
			if next_info != "a":
				info_text = info_text + "* " + next_info + "\n" + "\n"
			next_info = ""
		else:
			next_info += letter
	if next_info == "a":
		info_text = " "
	else:
		info_text = info_text + "* " + next_info + "\n" + "\n"
	info.text = info_text

