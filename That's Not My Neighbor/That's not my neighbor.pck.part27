func _play_sfx(n:int) -> void:
	match n:
		0:
			sfx_select.play()
		1:
			sfx_accept.play()
		2:
			sfx_pick_up_folder.play()
		3: 
			sfx_check_write.play()
		4:
			sfx_magic_dust.play()
		_:
			pass

func _set_summary() -> void:
	var points = 0
	doppel_label.text += str(Global.doppel_captured_level)
	doppel_label_2.text += str(Global.doppel_entered_level)
	doppel_label_3.text += str(Global.chars_killed_level)
	if Global.doppel_entered_level > 0:
		Global.days_killed_record += 1
		blood.visible = true
		if Global.achievements["BE_KILLED"] == 0:
			Global.achievements["BE_KILLED"] = 1
			achieve_unlock = true
	else:
		Global.days_successfully_complete_record += 1
		if Global.achievements["ALIVE"] == 0:
			Global.achievements["ALIVE"] = 1
			achieve_unlock = true
		if Global.days_successfully_complete_record >= 40:
			if Global.achievements["40_DAYS"] == 0:
				Global.achievements["40_DAYS"] = 1
				achieve_unlock = true
	if Global.chars_killed_level > 0:
		if Global.achievements["KILL_NEIGHBOR"] == 0:
			Global.achievements["KILL_NEIGHBOR"] = 1
			achieve_unlock = true
	# Set Rank
	var percentage_doppel = 0.0
	var percentage_chars = 0.0
	if Global.doppels_on_level != 0 and Global.chars_on_level != 0:
		percentage_doppel = float(Global.doppel_entered_level)/float(Global.doppels_on_level)
		percentage_chars = float(Global.chars_killed_level)/float(Global.chars_on_level)
	points += (percentage_doppel + percentage_chars)
	if points == 0:
		rank.text = "S"
		Global.S_rank_record += 1
		if Global.achievements["S_RANK"] == 0:
			Global.achievements["S_RANK"] = 1
			achieve_unlock = true
	elif points > 0 and points < 0.34:
		rank.text = "A"
		Global.A_rank_record += 1
	elif points >= 0.34 and points < 0.67:
		rank.text = "B"
		Global.B_rank_record += 1
	elif points >= 0.67 and points < 1:
		rank.text = "C"
		Global.C_rank_record += 1
	elif points >= 1 and points < 1.34:
		rank.text = "D"
		Global.D_rank_record += 1
	elif points >= 1.34 and points < 1.67:
		rank.text = "E"
		Global.E_rank_record += 1
	else:
		rank.text = "F"
		Global.F_rank_record += 1
		if Global.achievements["F_RANK"] == 0:
			Global.achievements["F_RANK"] = 1
			achieve_unlock = true
	Global.doppel_captured_record += Global.doppel_captured_level
	Global.doppel_entered_record += Global.doppel_entered_level
	Global.chars_killed_record += Global.chars_killed_level
	if Global.doppel_captured_record >= 100:
		if Global.achievements["100_DOPPELS"] == 0:
			Global.achievements["100_DOPPELS"] = 1
			achieve_unlock = true
	if Global.days_successfully_complete_record + Global.days_killed_record >= 100:
		if Global.achievements["100_DAYS"] == 0:
			Global.achievements["100_DAYS"] = 1
			achieve_unlock = true
	if !Global.did_folder_opened and !blood.visible:
		achieve_unlock = true
		if Global.achievements["NO_FOLDER"] == 0:
			Global.achievements["NO_FOLDER"] = 1
	Global.save_record()
	Global.save_achievements()
	Global.reset_values()

