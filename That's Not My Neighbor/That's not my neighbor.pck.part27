func _lineal_value_slider(value):
	if value == 0:
		return -80
	else:
		return 2*value/5 - 20

func _lineal_inverse_slider_value(value):
	if value == -80:
		return 0
	else:
		return 5*(value + 20)/2

@warning_ignore("unused_parameter")
func _on_circle_slider_01_drag_ended(value_changed) -> void:
